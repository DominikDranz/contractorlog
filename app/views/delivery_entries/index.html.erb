<p id="notice"><%= notice %></p>

<h1>Delivery Entries</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>In</th>
      <th>Out</th>
      <th>Company</th>
      <th>Plate vehicle</th>
      <th>Plate trailer</th>
      <th>Plate state</th>
      <th>Initials</th>
      <th>Pass num</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delivery_entries.each do |delivery_entry| %>
      <tr>
        <td><%= delivery_entry.date %></td>
        <td><%= delivery_entry.in %></td>
        <td><%= delivery_entry.out %></td>
        <td><%= delivery_entry.company %></td>
        <td><%= delivery_entry.plate_vehicle %></td>
        <td><%= delivery_entry.plate_trailer %></td>
        <td><%= delivery_entry.plate_state %></td>
        <td><%= delivery_entry.initials %></td>
        <td><%= delivery_entry.pass_num %></td>
        <td><%= link_to 'Show', delivery_entry %></td>
        <td><%= link_to 'Edit', edit_delivery_entry_path(delivery_entry) %></td>
        <td><%= link_to 'Destroy', delivery_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Delivery Entry', new_delivery_entry_path %>
